{
  "name": "kfo-web-admin-template",
  "private": true,
  "scripts": {
    "dev": "run-s start:dev",
    "start:dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "type-check": "tsc --noEmit",
    "format": "prettier --write ./src",
    "lint": "run-s lint:eslint lint:tsc",
    "lint:tsc": "tsc --noEmit",
    "lint:eslint": "eslint \"./src/**/*.{js,jsx,ts,tsx}\" --no-error-on-unmatched-pattern --cache --cache-location .eslintcache",
    "client": "run-s client:download client:build",
    "client:download": "node ./src/client/client.js",
    "client:build": "swagger-typescript-api --path ./src/client/api-docs.json --output ./src/@swagger --modular --clean-output --extract-request-params --no-client"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.20",
    "@mui/material": "^5.15.20",
    "aos": "^2.3.4",
    "axios": "^1.7.2",
    "formik": "^2.4.6",
    "framer-motion": "^11.2.12",
    "notistack": "^3.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.24.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/aos": "^3.0.7",
    "@types/node": "^20.14.8",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.5.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "14.2.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "prettier": "^3.3.2",
    "tailwindcss": "^3.4.4",
    "typescript": "5.5.2",
    "vite": "^5.3.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-tsconfig-paths": "^4.3.2"
  }
}
